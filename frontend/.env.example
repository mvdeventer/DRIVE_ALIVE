# Frontend Environment Variables
# Copy this file to .env and fill in your values
# DO NOT commit .env to git - it contains sensitive information

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:8000

# Debug Mode (Development Only)
# Set to 'true' to enable auto-fill in registration forms for faster testing
# MUST be 'false' in production
EXPO_PUBLIC_DEBUG_MODE=false

# Debug Credentials (Development Only - NEVER use in production)
# Leave empty to disable auto-fill
EXPO_PUBLIC_DEBUG_EMAIL=
EXPO_PUBLIC_DEBUG_PHONE=
EXPO_PUBLIC_DEBUG_PASSWORD=
EXPO_PUBLIC_DEBUG_STUDENT_EMAIL=
EXPO_PUBLIC_DEBUG_STUDENT_PHONE=
